{
  "name": "typed-document-node-example",
  "version": "0.1.0",
  "private": true,
  "author": "Alex Kondratyuk <lynxtaa@gmail.com>",
  "scripts": {
    "build": "tsc",
    "check-format": "pnpm run prettier --check",
    "check-types": "tsc",
    "format": "pnpm run prettier --write",
    "gql-codegen": "graphql-codegen --config graphql-codegen.yml",
    "lint": "eslint --ext js,ts src",
    "prettier": "prettier src",
    "start": "pnpm run build && node build/index.js"
  },
  "prettier": "@lynxtaa/prettier-config",
  "dependencies": {
    "awesome-graphql-client": "*",
    "graphql": "16.2.0",
    "node-fetch": "2.6.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.15.0",
    "@graphql-codegen/typed-document-node": "2.3.7",
    "@graphql-codegen/typescript": "2.8.2",
    "@graphql-codegen/typescript-operations": "2.5.7",
    "@graphql-typed-document-node/core": "3.1.1",
    "@lynxtaa/eslint-config": "0.5.3",
    "@lynxtaa/prettier-config": "0.1.1",
    "@types/node": "16.18.4",
    "@types/node-fetch": "2.5.12",
    "eslint": "8.28.0",
    "prettier": "2.8.0",
    "typescript": "4.9.3"
  }
}
