{
  "name": "typed-document-node-example",
  "version": "0.1.0",
  "private": true,
  "author": "Alex Kondratiuk <lynxtaa@gmail.com>",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "check-format": "pnpm run prettier --check",
    "check-types": "tsc",
    "format": "pnpm run prettier --write",
    "gql-codegen": "graphql-codegen --config graphql-codegen.ts",
    "lint": "eslint --max-warnings 0 src",
    "prettier": "prettier src",
    "start": "pnpm run build && node build/index.js"
  },
  "prettier": "@lynxtaa/prettier-config",
  "dependencies": {
    "awesome-graphql-client": "*",
    "graphql": "16.11.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.6",
    "@graphql-codegen/typed-document-node": "5.1.1",
    "@graphql-codegen/typescript": "4.1.6",
    "@graphql-codegen/typescript-operations": "4.6.1",
    "@graphql-typed-document-node/core": "3.2.0",
    "@lynxtaa/eslint-config": "catalog:",
    "@lynxtaa/prettier-config": "catalog:",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  }
}
