{
  "name": "awesome-graphql-client",
  "version": "0.9.0",
  "description": "GraphQL Client with file upload support for NodeJS and browser",
  "author": "Alex Kondratyuk <lynxtaa@gmail.com>",
  "repository": "https://github.com/lynxtaa/awesome-graphql-client.git",
  "homepage": "https://github.com/lynxtaa/awesome-graphql-client#readme",
  "license": "MIT",
  "keywords": [
    "graphql",
    "request",
    "graphql-client",
    "apollo"
  ],
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.m.js",
  "unpkg": "dist/index.umd.js",
  "simple-git-hooks": {
    "pre-commit": "npx pretty-quick --staged"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "rimraf dist/*",
    "build": "microbundle build --tsconfig tsconfig.build.json",
    "validate": "npm run check-format && npm run check-types && npm run lint",
    "lint": "eslint --ext .js,.ts src test",
    "prettier": "prettier \"{src,test}/**/*\" \"*.{js,json,md}\"",
    "check-format": "npm run prettier -- --check",
    "format": "npm run prettier -- --write",
    "check-types": "tsc",
    "release": "standard-version",
    "test": "is-ci-cli \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "7.13.12",
    "@babel/plugin-transform-modules-commonjs": "7.13.8",
    "@babel/preset-typescript": "7.13.0",
    "@graphql-typed-document-node/core": "3.1.0",
    "@types/extract-files": "8.1.0",
    "@types/jest": "26.0.22",
    "@types/node-fetch": "2.5.10",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "babel-jest": "26.6.3",
    "eslint": "7.24.0",
    "eslint-plugin-import": "2.22.1",
    "extract-files": "9.0.0",
    "form-data": "4.0.0",
    "graphql": "15.5.0",
    "graphql-tag": "2.11.0",
    "is-ci-cli": "2.2.0",
    "jest": "26.6.3",
    "microbundle": "0.13.0",
    "msw": "0.28.1",
    "node-fetch": "2.6.1",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "rimraf": "3.0.2",
    "simple-git-hooks": "^2.3.1",
    "standard-version": "9.2.0",
    "typescript": "4.2.4",
    "whatwg-fetch": "3.6.2"
  }
}
